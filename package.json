{
  "name": "bitewise",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "overrides": {
    "path-to-regexp": "^6.3.0",
    "js-yaml": "^4.1.1",
    "node-forge": "^1.3.2",
    "esbuild": "^0.24.2"
  },
  "scripts": {
    "dev": "vite",
    "build:server": "tsc -P backend-lib/tsconfig.server.json --pretty false && rm -rf backend-cjs api/backend-lib api/backend-cjs && mkdir -p backend-cjs api/backend-lib && cp -r dist/backend-lib/* backend-cjs/ && cp -r dist/backend-lib/* api/backend-lib/ && cp -r backend-cjs api/backend-cjs",
    "bundle:api": "true",
    "build": "npm run build:server && vite build",
    "vercel-build": "npm run build && npm run bundle:api",
    "start": "node dist/backend-lib/app.js",
    "lint": "eslint .",
    "test": "jest",
    "test:smoke": "jest --runInBand src/__tests__/ backend-lib/__tests__/pushFeedback.smoke.test.ts",
    "preview": "vite preview",
    "dev:server": "ts-node --project backend-lib/tsconfig.server.json backend-lib/index.ts",
    "vercel-deploy": "npm run build && vercel --prod",
    "check:assetlinks": "curl -I https://bitewise.co.in/.well-known/assetlinks.json"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@sentry/node": "^8.34.0",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.0.1",
    "@sentry/react": "^8.34.0",
    "firebase": "^12.5.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "morgan": "^1.10.1",
    "pino": "^9.1.0",
    "prom-client": "^15.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^2.15.10",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.4",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^1.0.2"
  }
}
